@model IEnumerable<OnChotto.Models.Entities.UsersFunctionDetail>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UsersId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FunctionId)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelitem => item.UsersId)
        </td>
        <td>
            @Html.DisplayFor(modelitem => item.FunctionId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { Id=item.Id}, new { @class = "btn btn-xs btn-warning" }) 
           
            @Html.ActionLink("Delete", "Delete", new { Id = item.Id }, new { @class = "btn btn-xs btn-danger", onclick = "return confirm('Bạn có chắc muốn xóa ')" })
        </td>
    </tr>
}

</table>
